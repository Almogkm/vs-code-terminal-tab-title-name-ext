{
  "name": "terminal-tab-titles",
  "displayName": "Terminal Tab Titles",
  "description": "Rename integrated terminal tabs based on executed commands.",
  "version": "0.1.0",
  "publisher": "local",
  "repository": {
    "type": "git",
    "url": "git@github.com:Almogkm/terminal-tab-titles.git"
  },
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": ["Productivity", "Other"],
  "keywords": [
    "terminal",
    "tab",
    "titles",
    "rename",
    "shell",
    "integrated terminal"
  ],
  "license": "MIT",
  "activationEvents": [
    "onStartupFinished",
    "onCommand:terminalTabTitles.debugInfo",
    "onCommand:terminalTabTitles.renameActiveTerminalTest",
    "onCommand:terminalTabTitles.revertActiveTerminalToBaseline"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Terminal Tab Titles",
      "properties": {
        "terminalTabTitles.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic terminal title updates based on executed commands."
        },
        "terminalTabTitles.debugLogging": {
          "type": "boolean",
          "default": false,
          "description": "Enable verbose debug logging to the Terminal Tab Titles output channel."
        }
      }
    },
    "commands": [
      {
        "command": "terminalTabTitles.debugInfo",
        "title": "Terminal Tab Titles: Debug Info"
      },
      {
        "command": "terminalTabTitles.renameActiveTerminalTest",
        "title": "Terminal Tab Titles: Rename Active Terminal Temporary Test"
      },
      {
        "command": "terminalTabTitles.revertActiveTerminalToBaseline",
        "title": "Terminal Tab Titles: Revert Active Terminal To Baseline"
      }
    ]
  }
}
